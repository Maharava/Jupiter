{
  "llm": {
    "provider": "ollama",
    "api_url": "http://localhost:11434",
    "default_model": "rocinante",
    "chat_temperature": 0.6,
    "extraction_temperature": 0.2,
    "token_limit": 8192
  },
  "chat": {
    "max_history_messages": 100
  },
  "paths": {
    "prompt_folder": "prompts",
    "logs_folder": "logs",
    "user_data_file": "user_data.json"
  },
  "ui": {
    "jupiter_color": "yellow",
    "user_color": "magenta"
  },
  "discord": {
    "enabled": false,
    "token": "",
    "allowed_servers": [],
    "allowed_channels": [],
    "observation_timeout": 300,
    "name_variations": [ "jupiter", "jup" ],
    "response_threshold": 0.7,
    "max_history_per_channel": 50,
    "user_mapping_file": "discord_users.json"
  },
  "calendar": {
    "enable_notifications": true,
    "notification_check_interval": 60,
    "prompt_detail_level": "normal",
    "default_reminder_minutes": 15
  },
  {
    "voice": {
      "enabled": true
    }
  }
}